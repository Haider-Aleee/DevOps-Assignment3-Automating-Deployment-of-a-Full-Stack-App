# AWS ECR Configuration
# Copy this file to .env and fill in your values

# ECR Registry URL (format: <account-id>.dkr.ecr.<region>.amazonaws.com)
ECR_REGISTRY=123456789012.dkr.ecr.us-east-1.amazonaws.com

# ECR Repository Names (just the repository name, not the full URI)
ECR_FRONTEND_REPO=react-node-frontend
ECR_BACKEND_REPO=react-node-backend

# Image tag to pull (defaults to 'latest' if not specified)
IMAGE_TAG=latest

# AWS Region
AWS_REGION=us-east-1
